<h1>Burgers!</h1>

<ul>
    {{#each burgers}}
    <li>
        {{this.Burger_name}} <button>devoured </button>
    </li>
    {{/each}}
</ul>

<h2>Name a Burger</h2>
<form class="create-form">

    <div class="form-group">
        <label for="ca">Burger Name:</label>
        <input type="text" id="bu" name="name">
    </div>

    <div>
        <button type="submit" class="burger">Add Burger</button>
    </div>

</form>